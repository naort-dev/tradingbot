{
    "name": "trality-landingpage",
    "version": "1.0.0",
    "sideEffects": [
        "./assets/**/*.*"
    ],
    "scripts": {
        "tsc": "tsc",
        "dev": "cross-env NODE_ENV=development next",
        "dev:prod": "cross-env NODE_ENV=production DEBUG=false next",
        "build": "next build && node ./sitemap_generator.js",
        "start": "next start",
        "vercel:login": "vercel login",
        "analyze": "cross-env ANALYZE=true next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "prettier": "prettier --write \"pages/**/*.{ts, tsx}\"",
        "export": "next export",
        "test": "serverless offline",
        "deploy": "vercel",
        "deploy:production": "vercel --prod",
        "link-comps-package-locally": "yarn add file:../web-ui-components && rm -rf ./node_modules/@trality/web-ui-components/node_modules/react && rm -rf ./node_modules/@trality/web-ui-components/node_modules/react-dom"
    },
    "dependencies": {
        "@babel/preset-typescript": "^7.14.5",
        "@sentry/webpack-plugin": "^1.13.0",
        "@svgr/webpack": "^5.5.0",
        "@trality/web-tracking": "6.0.3",
        "@trality/web-ui-components": "0.0.51",
        "@zeit/next-css": "^1.0.1",
        "dotenv": "^8.2.0",
        "express": "^4.16.4",
        "isomorphic-fetch": "^2.2.1",
        "jsonp": "^0.2.1",
        "next": "^9.0.7",
        "next-transpile-modules": "4",
        "nextjs-sitemap-generator": "^1.1.3",
        "path-match": "^1.2.4",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-ga": "^2.5.7",
        "react-gtm-module": "^2.0.8",
        "react-svg": "^10.0.22",
        "react-waypoint": "^9.0.1",
        "styled-components": "^5.1.1",
        "svg-url-loader": "^7.1.1",
        "ts-loader": "^9.2.4",
        "use-media": "^1.4.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/helper-validator-identifier": "^7.10.3",
        "@emotion/core": "^10.0.28",
        "@emotion/styled": "^10.0.27",
        "@next/bundle-analyzer": "^9.5.5",
        "@types/express": "^4.16.1",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/jsonp": "^0.2.0",
        "@types/mixpanel-browser": "^2.23.0",
        "@types/react": "^16.8.12",
        "@types/react-dom": "^16.8.3",
        "@types/react-gtm-module": "^2.0.0",
        "@types/styled-components": "^5.1.1",
        "@zeit/next-source-maps": "^0.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-inline-react-svg": "^2.0.1",
        "babel-plugin-styled-components": "^1.10.0",
        "cross-env": "^7.0.2",
        "emotion-normalize": "^10.1.0",
        "emotion-theming": "^11.0.0",
        "escape-html": "^1.0.3",
        "file-loader": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "fs-extra": "^8.1.0",
        "husky": "^4.2.5",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.1",
        "markdown-it": "^10.0.0",
        "moment": "^2.27.0",
        "next-optimized-images": "^2.4.0",
        "react-svg-loader": "^3.0.3",
        "sharp": "^0.23.1",
        "svg-inline-loader": "^0.8.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.4.2",
        "typescript-plugin-styled-components": "^1.4.2",
        "vercel": "^21.0.1"
    },
    "license": "ISC",
    "engines": {
        "node": "12.x"
    }
}
